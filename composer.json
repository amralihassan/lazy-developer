{
    "name": "bakery-soft/crud-generator",
    "description": "Create Dashboard and auto generate crud",    
    "license": "MIT",
    "authors": [
        {
            "name": "BakerySoft Company",
            "email": "amr.meghaed@bakerysoft.net",
            "homepage": "https://.bakerysoft.net"
        }
    ],
    "minimum-stability": "stable",
    "keywords": [
        "Laravel",        
        "crud",
        "crud generator",
        "laravel crud generator"
    ],
    "require": {
        "php": ">=5.4.0"        
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BakerySoft\\LaravelBakerySoft\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BakerySoft\\LaravelBakerySoft\\LaravelBakerySoftServiceProvider"
            ],
            "aliases": {
                "LaravelBakerySoft": "BakerySoft\\LaravelBakerySoft\\Facades\\LaravelBakerySoft"
            }
        }
    }
}
